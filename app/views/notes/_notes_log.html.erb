<div class="notes-log">
  <table class="table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Commentor</th>
        <th>Note</th>
      </tr>
    </thead>
    <tbody>
      <% support_request.notes.each do |note| %>
        <tr>
          <td><%= note.eff_date %></td>
          <td><%= note.author %></td>
          <td><%= note.content %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
